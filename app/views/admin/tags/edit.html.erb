<div class="container">
  <div class="row">
    <div>
      <%= form_with model: @tag, url: admin_tags_path(@tag.id), method: :patch do |f| %>
      <div class="field">
        <%= f.label :タグ %>
        <%= f.text_field :tagname, placeholder: "タグ名" %>
        <%= f.submit, "更新", class: "btn btn-primary" %>
      </div>
      <% end %>
    </div>
  </div>
</div>